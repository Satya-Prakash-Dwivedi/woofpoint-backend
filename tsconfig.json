        "target": "ES2020",
        "module": "commonjs",
        "rootDir": ".",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "server.ts",
        "controllers/**/*.ts",
        "middlewares/**/*.ts",
        "types/**/*.d.ts"
    ]
}